{
  "name": "node-typescript-starter",
  "version": "1.0.0",
  "description": "Node project starter with Typescript, ESLint and Prettier",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch \"*.ts\" --exec \"ts-node\" ./src/index.ts",
    "eslint:fix": "eslint --fix",
    "soundcloud": "ts-node ./src/scrape-soundclound.ts",
    "zillow": "ts-node ./src/scrape-zillow.ts"
  },
  "devDependencies": {
    "@types/node": "^18.15.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "axios-https-proxy-fix": "^0.17.1",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.0.3",
    "puppeteer-core": "^20.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  }
}
